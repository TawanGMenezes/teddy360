{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"partner": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/partner",
			"sourceRoot": "projects/partner/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/partner",
						"index": "projects/partner/src/index.html",
						"main": "projects/partner/src/main.ts",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/partner/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/partner/src/favicon.ico",
							"projects/partner/src/assets"
						],
						"styles": [
							"projects/partner/src/styles.scss",
							"@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/primeflex/primeflex.css",
              "node_modules/ngx-spinner/animations/ball-scale-multiple.css"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/partner/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/partner/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "partner:build:production",
							"extraWebpackConfig": "projects/partner/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "partner:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4201,
						"publicHost": "http://localhost:4201",
						"extraWebpackConfig": "projects/partner/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "partner:build",
						"extraWebpackConfig": "projects/partner/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/partner/tsconfig.spec.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/partner/src/favicon.ico",
							"projects/partner/src/assets"
						],
						"styles": [
							"projects/partner/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		},
