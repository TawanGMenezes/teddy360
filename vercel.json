{
  "version": 2,
  "builds": [
    {
      "src": "projects/core//*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/core"
      }
    },
    {
      "src": "projects/partner//*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/partner"
      }
    },
    {
      "src": "projects/external-companies//*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/external-companies"
      }
    },
    {
      "src": "projects/list-partner//*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/list-partner"
      }
    },
    {
      "src": "projects/list-external-companies//*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/list-external-companies"
      }
    },
    {
      "src": "projects/about//*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/about"
      }
    }
  ],
  "routes": [
    { "src": "/core/(.*)", "dest": "/projects/core/dist/$1" },
    { "src": "/partner/(.*)", "dest": "/projects/partner/dist/$1" },
    { "src": "/external-companies/(.*)", "dest": "/projects/external-companies/dist/$1" },
    { "src": "/list-partner/(.*)", "dest": "/projects/list-partner/dist/$1" },
    { "src": "/list-external-companies/(.*)", "dest": "/projects/list-external-companies/dist/$1" },
    { "src": "/about/(.*)", "dest": "/projects/about/dist/$1"}
  ]
}
