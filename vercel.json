{
  "routes": [
    {
      "src": "/core/*",
      "dest": "/projects/core/dist"
    },
    {
      "src": "/partner/*",
      "dest": "/projects/partner/dist"
    },
    {
      "src": "/external-companies/*",
      "dest": "/projects/external-companies/dist"
    },
    {
      "src": "/list-partner",
      "dest": "/projects/list-partner/dist"
    },
    {
      "src": "/list-external-companies",
      "dest": "/projects/list-external-companies/dist"
    },
    {
      "src": "/about",
      "dest": "/projects/about/dist"
    }
  ],

    "builds": [
      {
        "src": "core",
        "use": "@vercel/node",
        "buildCommand": "cd core && ng build --prod"
      },
      {
        "src": "partner",
        "use": "@vercel/node",
        "buildCommand": "cd partner && ng build --prod"
      },
      {
        "src": "external-companies",
        "use": "@vercel/node",
        "buildCommand": "cd external-companies && ng build --prod"
      },
      {
        "src": "list-partner",
        "use": "@vercel/node",
        "buildCommand": "cd list-partner && ng build --prod"
      },
      {
        "src": "list-external-companies",
        "use": "@vercel/node",
        "buildCommand": "cd list-external-companies && ng build --prod"
      },
      {
        "src": "about",
        "use": "@vercel/node",
        "buildCommand": "cd about && ng build --prod"
      }
    ]

}
