{
  "routes": [
    {
      "src": "/core/*",
      "dest": "/projects/core/dist"
    },
    {
      "src": "/partner/*",
      "dest": "/projects/partner/dist"
    },
    {
      "src": "/external-companies/*",
      "dest": "/projects/external-companies/dist"
    },
    {
      "src": "/list-partner",
      "dest": "/projects/list-partner/dist"
    },
    {
      "src": "/list-external-companies",
      "dest": "/projects/list-external-companies/dist"
    },
    {
      "src": "/about",
      "dest": "/projects/about/dist"
    }
  ],
  "builds": [
    {
      "src": "/projects/core",
      "use": "@vercel/node",
      "buildCommand": "npm install && ng build --prod"
    },
    {
      "src": "/projects/partner",
      "use": "@vercel/node",
      "buildCommand": "npm install && ng build --prod"
    },
    {
      "src": "/projects/external-companies",
      "use": "@vercel/node",
      "buildCommand": "npm install && ng build --prod"
    },
    {
      "src": "/projects/list-partner",
      "use": "@vercel/node",
      "buildCommand": "npm install && ng build --prod"
    },
    {
      "src": "/projects/list-external-companies",
      "use": "@vercel/node",
      "buildCommand": "npm install && ng build --prod"
    },
    {
      "src": "/projects/about",
      "use": "@vercel/node",
      "buildCommand": "npm install && ng build --prod"
    }
  ]
}
